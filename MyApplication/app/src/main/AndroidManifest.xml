<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.myapplication">
    <!--注册联网权限-->
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/weifencloud"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SensorRegistrationActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_sensor_registration"></activity>
        <activity
            android:name=".CloudManagerActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_cloud_manager"></activity>
    </application>

</manifest>
